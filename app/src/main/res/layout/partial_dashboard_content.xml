<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <LinearLayout
        android:id="@+id/ll_new_arrivals_content"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_margin="@dimen/dimen_8dp"
            android:orientation="vertical">

            <LinearLayout
                android:id="@+id/ll_new_arrivals_content_top"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:orientation="horizontal">


            </LinearLayout>

            <LinearLayout
                android:id="@+id/ll_new_arrivals_content_bottom"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/dimen_10dp"
                android:gravity="center"
                android:orientation="horizontal">


            </LinearLayout>
        </LinearLayout>

        <com.webmyne.rentalapp.custom.TfTextView
            android:id="@+id/tv_no_prev_new_arrivals"
            android:layout_width="match_parent"
            android:layout_height="@dimen/height_200"
            android:gravity="center"
            android:text="@string/str_no_new_arrivals"
            android:textSize="@dimen/sp12"
            android:visibility="gone" />

    </LinearLayout>

</LinearLayout>
